                  <p class="card-title">Personnel on Queue</p>
                  <div class="row">
                    <div class="col-12">
                      <div class="table-responsive">
                        <table class="display expandable-table" style="width:100%">
                          <thead>
                            <tr>
                              <th>#</th>
                              <th>Name</th>
                              <th>Queue No</th>
                              <th>Status</th>
                              <th>Before</th>
                              <th>Current</th>
                              <th>After</th>
                              <th>Date In</th>
                              <th>Time In</th>
                              <% if (user.role === 3) { %>
                                <th>Date Out</th>
                                <th>Time Out</th>
                                <th>Attendant</th>
                              <% } %>
                              
                              <th></th>
                            </tr>
                          </thead>
                      </table>
                      </div>
                    </div>
                  </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
